@model IEnumerable<DataLayer.Ticket>



@{
    ViewBag.Title = "showTicNewsByID";
}




<ul class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">خانه </a></li>
    <li class="breadcrumb-item"><a href="#">اخبار </a></li>
    <li class="breadcrumb-item active"> @ViewBag.name</li>
</ul>


<section class="last-news-section border-radius">
    <header><i class="icons icon-doc-2"></i>  <h3> آرشیو تیتکهای @ViewBag.name</h3> </header>
    <ul>


        @foreach (var item in Model)
        {

            <li>
                <img class="border-radius" src="~/TicketImages/@item.ImageName">
                <h2> <a href="/TicNews/@item.TicketID"> @item.Title </a></h2>
                <p>
                    @item.Discription
                </p>
                <span class="date-publish"><i class="fa fa-calendar"></i> انتشار : @item.CreateDate.ToShamsi() </span>
                <a href="/TicNews/@item.TicketID" class="more-link"><i class="icons icon-more"></i></a>
            </li>

        }



    </ul>
</section>




